#!/usr/bin/env symon -dnf
## BEGIN_INPUT
# init 0
# check 0 1
# init 2
# add 1 3
# add 2 4
# add 3 5
# add 4 6
# check 10 7
## END_INPUT
## BEGIN_OUTPUT
# @1.000000.	(time-point 1)	x0 == 0
# @7.000000.	(time-point 7)	x0 == 10
## END_OUTPUT

var {
    v: number;
}

signature init {}
signature add {
    n: number;
}
signature check {
    n: number;
}

(init() || add() || check())*;
init( | | v := 0 );
add( n | | v := n + 0 + v )*;
check( n | v = n )
